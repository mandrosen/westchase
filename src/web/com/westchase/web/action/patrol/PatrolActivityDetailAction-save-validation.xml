<!DOCTYPE validators PUBLIC
        "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
        "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="receivedTime">
        <field-validator type="requiredstring">
            <message><![CDATA[ Received time is required ]]></message>
        </field-validator>
	</field>
	<field name="arrivedTime">
        <field-validator type="requiredstring">
            <message><![CDATA[ Arrived time is required ]]></message>
        </field-validator>
	</field>
	<field name="clearedTime">
        <field-validator type="requiredstring">
            <message><![CDATA[ Cleared time is required ]]></message>
        </field-validator>
	</field>
	<field name="currentPatrolActivityDetail.patrolDetailCategory.id">
	    <field-validator type="int">
	        <param name="min">1</param>
	        <message><![CDATA[ Category is required ]]></message>
	    </field-validator>
	</field>
	<field name="currentPatrolActivityDetail.patrolDetailType.id">
	    <field-validator type="int">
	        <param name="min">1</param>
	        <message><![CDATA[ Type is required ]]></message>
	    </field-validator>
	</field>
</validators>
